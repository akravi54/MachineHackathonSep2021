      "cell_type": "code",
      "metadata": {
        "id": "xaE4QkYYr8Ms"
      },
      "source": [
        "import tensorflow.keras as tf"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "EsUVr1RusAwD"
      },
      "source": [
        "### build the model (Create the object for model)\n",
        "model = tf.models.Sequential()  ## blank model\n",
        "## Adding the layers\n",
        "model.add(tf.layers.Dense(16,input_dim=14,activation=\"relu\"))  ## two layers - input layer,hidden\n",
        "model.add(tf.layers.Dense(32,activation='relu'))  ## 2nd hidden layer\n",
        "model.add(tf.layers.Dense(1,activation='sigmoid'))   ## output layer\n",
        "## Compile the model\n",
        "model.compile(loss=\"binary_crossentropy\",optimizer=\"adam\",metrics=[\"accuracy\"])"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "nTQA6L-MvvTv",
        "outputId": "202b96f8-bd82-44be-eebe-145c36f6ad36"
      },
      "source": [
        "### Train the model\n",
        "model.fit(xtrain,ytrain,epochs=500)"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            "Epoch 1/500\n",
            "15/15 [==============================] - 1s 2ms/step - loss: 0.6521 - accuracy: 0.6242\n",
            "Epoch 2/500\n",
            "15/15 [==============================] - 0s 2ms/step - loss: 0.5778 - accuracy: 0.8088\n",
            "Epoch 3/500\n",
            "15/15 [==============================] - 0s 2ms/step - loss: 0.5097 - accuracy: 0.8857\n",
            "Epoch 4/500\n",
            "15/15 [==============================] - 0s 2ms/step - loss: 0.4428 - accuracy: 0.9077\n",
            "Epoch 5/500\n",
